<mvc:View controllerName="aymax.pfe.inventaire.controller.Home" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc">
	<Page id="_IDGenPage1" showHeader="false">
		<landmarkInfo>
			<PageAccessibleLandmarkInfo id="_IDGenPageAccessibleLandmarkInfo1" rootRole="Region" rootLabel="{i18n>Home_rootLabel}" contentRole="Main" contentLabel="{i18n>Home_contentLabel}"/>
		</landmarkInfo>
		<l:BlockLayout id="BlockLayout" background="Default">
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow1">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell1" class="sapUiNoContentPadding">
					<Image id="_IDGenImage1" src="{ parts: [{path:'view>/isPhone'}, {path:'i18n>homeImage'}], formatter: '.formatter.srcImageValue' }" width="100%" height="100%"/>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow2">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell2" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout1">
						<core:Icon id="_IDGenIcon1" src="sap-icon://role" size="2.5rem" color="Default"/>
						<Title id="administrationTile" text="{i18n>administrationTitle}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText1" text="{i18n>administrationContent}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink1" text="{i18n>accessLink}" tooltip="{i18n>accessAdministrationLink}"/>
						<Link id="_IDGenLink2" text="{i18n>learnLink}" tooltip="{i18n>learnAdministrationTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell3" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeB">
					<l:VerticalLayout id="_IDGenVerticalLayout2">
						<core:Icon id="_IDGenIcon2" src="sap-icon://add-product" size="2.5rem" color="Default"/>
						<Title id="productTile" text="{i18n>productTitle}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText2" text="{i18n>productContent}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink3" text="{i18n>accessLink}" tooltip="{i18n>accessProductsLink}"/>
						<Link id="_IDGenLink4" text="{i18n>learnLink}" tooltip="{i18n>learnProductsTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell4" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout3">
						<core:Icon id="_IDGenIcon3" src="sap-icon://customer-financial-fact-sheet" size="2.5rem" color="Default"/>
						<Title id="_IDGenTitle1" text="{i18n>revenueTitle}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText3" text="{i18n>revenueContent}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink5" text="{i18n>accessLink}" tooltip="{i18n>accessRevenueLink}"/>
						<Link id="_IDGenLink6" text="{i18n>learnLink}" tooltip="{i18n>learnRevenueTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell5" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeB">
					<l:VerticalLayout id="_IDGenVerticalLayout4">
						<core:Icon id="supplierIcon" src="sap-icon://supplier" size="2.5rem" color="Default"/>
						<Title id="_IDGenTitle2" text="{i18n>supplierTitle}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText4" text="{i18n>supplierContent}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink7" text="{i18n>accessLink}" tooltip="{i18n>accessSuppliersLink}"/>
						<Link id="_IDGenLink8" text="{i18n>learnLink}" tooltip="{i18n>learnSuppliersTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow3">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell6" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeB">
					<l:VerticalLayout id="_IDGenVerticalLayout5">
						<core:Icon id="_IDGenIcon4" src="sap-icon://thumb-up" size="2.5rem" color="Default"/>
						<Title id="_IDGenTitle3" text="{i18n>socialMediaTitle}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText5" text="{i18n>socialMediaContent}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink9" text="{i18n>accessLink}" tooltip="{i18n>accessSocialLink}"/>
						<Link id="_IDGenLink10" text="{i18n>learnLink}" tooltip="{i18n>learnSocialMediaTooltip}"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell7" backgroundColorSet="ColorSet9" backgroundColorShade="ShadeA" width="3">
					<Table id="_IDGenTable1" items="{path:'customer>/customers', templateShareable: false}" fixedLayout="false">
						<headerToolbar>
							<Toolbar id="_IDGenToolbar1" design="Transparent">
								<Title id="_IDGenTitle4" text="{i18n>customerOverviewTitle}" level="H3"/>
							</Toolbar>
						</headerToolbar>
						<columns>
							<Column id="_IDGenColumn1">
								<Text id="_IDGenText6" text="{i18n>customerName}"/>
							</Column>
							<Column id="_IDGenColumn2" minScreenWidth="Tablet">
								<Text id="_IDGenText7" text="{i18n>productId}"/>
							</Column>
							<Column id="_IDGenColumn3" minScreenWidth="Desktop" hAlign="End">
								<Text id="_IDGenText8" text="{i18n>lastPurchase}"/>
							</Column>
							<Column id="_IDGenColumn4" hAlign="Center">
								<Text id="_IDGenText9" text="{i18n>payment}"/>
							</Column>
							<Column id="_IDGenColumn5" hAlign="End">
								<Text id="_IDGenText10" text="{i18n>amount}"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem id="_IDGenColumnListItem1">
								<cells>
									<Text id="_IDGenText11" text="{customer>name}"/>
									<Text id="_IDGenText12" text="{customer>productId}"/>
									<Text id="_IDGenText13" text="{customer>lastPurchase}"/>
									<ObjectNumber id="_IDGenObjectNumber1" number="{customer>payment}" state="{customer>state}"/>
									<ObjectNumber id="_IDGenObjectNumber2"
										number="{ parts:[{path:'customer>amount'},{path:'customer>currencyCode'}], type: 'sap.ui.model.type.Currency', formatOptions: {showMeasure: false} }"
										unit="{customer>currencyCode}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
		</l:BlockLayout>
	</Page>
</mvc:View>